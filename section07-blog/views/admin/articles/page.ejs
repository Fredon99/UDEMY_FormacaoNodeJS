<!DOCTYPE html>
<html lang="pt-br">
<%- include('../../partials/head.ejs'); %>
<body>
    <%- include('../../partials/homeNavbar.ejs'); %>
    <br>
    <div class="container">

        <% result.articles.rows.forEach( (article) => { %>
            <div class="card-header">
                 <h3> <%= article.title %> </h3>
            </div>
            <div class="card-body">
                <a href="/<%= article.slug%>" class="btn btn-dark"> Ler artigo </a>
            </div>
            <br>
        <% }) %>

    </div>
</body>
<%- include('../../partials/script.ejs'); %>
</html>